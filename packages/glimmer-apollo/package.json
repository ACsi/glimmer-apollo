{
  "name": "glimmer-apollo",
  "version": "0.4.1",
  "description": "Ember and Glimmer integration for Apollo Client",
  "keywords": [
    "ember-addon",
    "glimmer",
    "apollo",
    "graphql"
  ],
  "repository": "http://github.com/josemarluedke/glimmer-apollo",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*",
    "./addon-main.js": "./addon-main.js"
  },
  "typings": "dist/index.d.ts",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w",
    "test": "echo 'TODO'",
    "test:types": "tsc --noEmit",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@ember/test-waiters": "^3.0.0",
    "@embroider/addon-shim": "^0.44.1",
    "@glimmer/env": "^0.1.7",
    "@wry/equality": "^0.5.2"
  },
  "devDependencies": {
    "@apollo/client": "3.4.13",
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.15.4",
    "@babel/preset-env": "^7.15.6",
    "@babel/runtime": "^7.15.4",
    "@embroider/addon-dev": "^1.0.0",
    "@wessberg/rollup-plugin-ts": "^1.3.14",
    "graphql": "^15.6.0",
    "rollup": "^2.57.0",
    "typescript": "^4.4.3",
    "walk-sync": "^3.0.0"
  },
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "@glimmer/destroyable": "^0.77.6",
    "@glimmer/manager": "^0.77.6",
    "@glimmer/owner": "^0.77.6",
    "@glimmer/runtime": "^0.77.6",
    "@glimmer/tracking": "^2.0.0-beta.15",
    "@glimmer/validator": "^0.77.6",
    "ember-source": "^3.27.0",
    "graphql": "^14.0.0 || ^15.0.0"
  },
  "peerDependenciesMeta": {
    "ember-source": {
      "optional": true
    },
    "@glimmer/destroyable": {
      "optional": true
    },
    "@glimmer/manager": {
      "optional": true
    },
    "@glimmer/owner": {
      "optional": true
    },
    "@glimmer/validator": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "ember-addon-main.js",
    "app-js": {
      "./initializers/set-glimmer-apollo-environment-ember.js": "./dist/ember-initializer.js"
    }
  },
  "volta": {
    "node": "14.17.0"
  }
}
