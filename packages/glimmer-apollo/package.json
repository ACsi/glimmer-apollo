{
  "name": "glimmer-apollo",
  "version": "0.1.0",
  "description": "Ember and Glimmer integration for Apollo Client",
  "keywords": [
    "ember-addon",
    "glimmer",
    "apollo",
    "graphql"
  ],
  "repository": "http://github.com/josemarluedke/glimmer-apollo",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "module": "./dist/esm/index.js"
    },
    "./environment": {
      "require": "./dist/cjs/environment.js",
      "module": "./dist/esm/environment.js"
    },
    "./environment-glimmer": {
      "require": "./dist/cjs/environment-glimmer.js",
      "module": "./dist/esm/environment-glimmer.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "rollup -c rollup.config.js && yarn build:types",
    "build:types": "tsc --noEmit false --declaration --declarationDir ./dist/types --emitDeclarationOnly --allowJs false && rm -rf ./dist/types/tests",
    "start": "rollup -c rollup.config.js -w",
    "test": "echo 'TODO'",
    "test:types": "tsc --noEmit",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@ember/test-waiters": "^2.4.3",
    "@embroider/addon-shim": "^0.41.0",
    "@glimmer/destroyable": "^0.77.6",
    "@glimmer/env": "^0.1.7",
    "@glimmer/manager": "^0.77.6",
    "@glimmer/owner": "^0.77.6",
    "@glimmer/runtime": "^0.77.6",
    "@glimmer/tracking": "^2.0.0-beta.15",
    "@glimmer/validator": "^0.77.6",
    "@wry/equality": "^0.4.0"
  },
  "devDependencies": {
    "@apollo/client": "3.3.19",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "rollup": "^2.50.0",
    "rollup-plugin-multi-input": "^1.3.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "graphql": "^14.0.0 || ^15.0.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "ember-addon": {
    "version": 2,
    "type": "addon",
    "main": "ember-addon-main.js",
    "app-js": {
      "./initializers/set-glimmer-apollo-environment-ember.js": "./dist/esm/environment-ember.js"
    }
  },
  "volta": {
    "node": "14.17.0"
  }
}
