{
  "name": "glimmer-apollo",
  "version": "0.0.2-beta.1",
  "description": "Ember and Glimmer integration for Apollo Client",
  "keywords": [
    "ember-addon",
    "glimmer",
    "apollo",
    "graphql"
  ],
  "repository": "http://github.com/josemarluedke/glimmer-apollo",
  "license": "MIT",
  "author": "Josemar Luedke <josemarluedke@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "main": "build/commonjs/addon/index.js",
  "module": "build/modules/addon/index.js",
  "typings": "build/types/addon/index.d.ts",
  "exports": {
    ".": {
      "require": "./build/commonjs/addon/index.js",
      "module": "./build/modules/addon/index.js"
    },
    "./environment-glimmer": {
      "require": "./build/commonjs/addon/environment-glimmer.js",
      "module": "./build/modules/addon/environment-glimmer.js"
    },
    "./environment-ember": {
      "require": "./build/commonjs/addon/environment-ember.js",
      "default": "./build/modules/addon/environment-ember.js"
    }
  },
  "scripts": {
    "prebuild": "rm -rf ./build",
    "build": "node ../../scripts/build.js && yarn build:types",
    "build:types": "tsc --noEmit false --declaration --declarationDir ./build/types --emitDeclarationOnly --allowJs false && rm -rf ./build/types/tests",
    "start": "ember serve",
    "test": "ember test",
    "test:types": "tsc --noEmit",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@apollo/client": "^3.3.13",
    "@ember/test-waiters": "^2.4.3",
    "@glimmer/destroyable": "^0.77.6",
    "@glimmer/env": "^0.1.7",
    "@glimmer/owner": "^0.77.6",
    "@glimmer/runtime": "^0.77.6",
    "@glimmer/tracking": "^2.0.0-beta.15",
    "@glimmer/validator": "^0.77.6",
    "@wry/equality": "^0.4.0",
    "ember-auto-import": "^1.10.1",
    "ember-cli-babel": "^7.26.2",
    "ember-cli-typescript": "^4.1.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.0",
    "@glimmer/component": "^1.0.3",
    "@types/ember-qunit": "^3.4.13",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "^3.16.2",
    "@types/ember__array": "^3.16.4",
    "@types/ember__component": "^3.16.4",
    "@types/ember__controller": "^3.16.4",
    "@types/ember__debug": "^3.16.3",
    "@types/ember__engine": "^3.16.2",
    "@types/ember__error": "^3.16.1",
    "@types/ember__object": "^3.12.5",
    "@types/ember__polyfills": "^3.12.1",
    "@types/ember__routing": "^3.16.11",
    "@types/ember__runloop": "^3.16.3",
    "@types/ember__service": "^3.16.1",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^3.16.1",
    "@types/ember__test": "^3.16.1",
    "@types/ember__test-helpers": "^1.7.3",
    "@types/ember__utils": "^3.16.2",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "@types/sinon": "^9.0.11",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.25.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.1",
    "ember-qunit": "^5.1.2",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.25.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.4.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "loader.js": "^4.7.0",
    "msw": "^0.27.1",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.14.0",
    "qunit-dom": "^1.6.0",
    "sinon": "^10.0.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "graphql": "^14.0.0 || ^15.0.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "main": "ember-addon-main.js"
  },
  "msw": {
    "workerDirectory": "tests/dummy/public"
  }
}
